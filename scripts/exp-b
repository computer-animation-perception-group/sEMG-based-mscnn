ver=1.0.0.0
python -m sigr.train_feature exp --log log --snapshot model \
  --root .cache/ninapro-db1-actimg-15-1-universal-one-fold-intra-subject \
  --batch-size 1000 --decay-all --dataset ninapro-db1-var-raw-prepro-lowpass \
  --num-filter 64 \
  --num-epoch 28 --lr-step 16 --lr-step 24 --snapshot-period 28 \
  --balance-gesture 1 \
  --num-semg-row 15 --num-semg-col 50 \
  crossval --crossval-type universal-one-fold-intra-subject --fold 0 


ver=1.0.0.1
for i in $(seq 0 26); do
   python -m sigr.train_feature exp --log log --snapshot model \
   --root .cache/ninapro-db1-actimg-15-1-one-fold-intra-subject-fold-$i-v$ver \
   --batch-size 1000 --decay-all --dataset ninapro-db1-var-raw-prepro-lowpass \
   --params .cache/ninapro-db1-actimg-15-1-universal-one-fold-intra-subject/model-0028.params \
   --num-filter 64 \
   --num-epoch 28 --lr-step 16 --lr-step 24 --snapshot-period 28 \
   --balance-gesture 1 \
   --num-semg-row 15 --num-semg-col 50 \
   crossval --crossval-type one-fold-intra-subject --fold $i 
done    